@model List<Item>

<h2>Edit Items</h2>

<ul>
    @foreach (var item in Model)
    {
        <li>
            <a asp-action="Edit" asp-route-id="@item.Id">@item.Name</a>
        </li>
    }
</ul>

<div>
    <a asp-action="Index">Back to List</a> |
    <a asp-action="Create">Create New Item</a> |
    <a asp-action="Delete">Delete Item</a>
</div>